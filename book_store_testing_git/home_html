from selenium import webdriver

driver = webdriver.Chrome(executable_path='C:/chromedriver')
driver.maximize_window()
driver.get("http://practice.automationtesting.in/")
driver.execute_script("window.scrollBy(0, 600);")
selenium_ruby = driver.find_element_by_css_selector("#text-22-sub_row_1-0-2-0-0 > div > ul > li > a.woocommerce-LoopProduct-link > h3").click()
reviews = driver.find_element_by_class_name("reviews_tab").click()
star_5 = driver.find_element_by_class_name("star-5").click()
review = driver.find_element_by_id("comment")
review.send_keys("Nice book!")
name = driver.find_element_by_id("author")
name.send_keys("NAME")
email = driver.find_element_by_id("email")
email.send_keys("email@email.ru")
sumbit = driver.find_element_by_id("submit").click()
driver.quit()